<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav" style="align-items:center">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
        <% if (email) {%>
            <form method="POST" action="/logout" class="form-inline" style="position: absolute; right: 30px;">
              <label>Logged In As <%= email %>ÔºÅ  </label>
              <button>Logout</button>
            </form>
        <% } else {%>
            <div class="rightdisplay form-inline" style="display: flex; position: absolute; right: 30px; ">
              <form method="GET" action="/Register" class="form-inline">
                <button class="login">Register</button>
              </form>
              <form method="GET" action="/login" class="form-inline">
                <button class="login">Login</button>   
              </form>
            </div>
        <% }%>
      </div>
    </div>
  </nav>
</header>